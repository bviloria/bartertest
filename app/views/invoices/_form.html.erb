<%= simple_form_for(@invoice) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :id %>
    <%= f.input :terminal_id %>
    <%= f.input :transaction_id %>
    <%= f.input :origin_invoice_id %>
    <%= f.input :site_code %>
    <%= f.input :transaction_code %>
    <%= f.input :type_code %>
    <%= f.input :document_no %>
    <%= f.input :status %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
